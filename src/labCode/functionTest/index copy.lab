(define-syntax class 
    (syntax-rules ()
        (
            (_ 
                name 
                (constructor (...) ...) 
            ) 
            (begin 
                (define name 
                        (lambda (...)
                            (
                                (lambda ()
                                ...
                                )
                            )
                        )
                )
                name
            )     
        )       
    )
)

;定义类
(class animal
    (constructor (name_num mouse_num) 
        (define (. this "name") name_num)
        (define (. this "mouse") mouse_num)
    )
)

(animal 1 2)


